@model List<Category>
<h1>Category List</h1>

<table>
    <tr>
        <th>Category Name</th>
        <th>Display Order</th>
    </tr>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>